        </media>
      </figure>
      <para id="id3149249">1.3</para>
      <figure id="id3149257">
        <media id="id3149257_media" alt="">
          <image mime-type="image/png" src="../../media/Picture 7-9254.png" id="id3149257__onlineimage" height="60" width="60"/>
        </media>
