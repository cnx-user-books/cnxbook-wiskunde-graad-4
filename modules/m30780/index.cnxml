            <row>
              <entry>Hele blokkies</entry>
              <entry>Halwe blokkies wat tot heles gemaak is</entry>
              <entry>Ander dele wat tot hele blokke gemaak is</entry>
              <entry>Totale getal blokkies wat deur my hand bedek word.</entry>
            </row>
            <row>
              <entry/>
              <entry/>
              <entry/>
              <entry/>
            </row>
          </tbody>
        </tgroup>
