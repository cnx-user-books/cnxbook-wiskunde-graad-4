              <entry>Aantal suikerblokkies wat nodig was om die kartondosie te vul</entry>
            </row>
            <row>
              <entry>Vuurhoutjiedosie</entry>
              <entry/>
            </row>
            <row>
              <entry/>
              <entry/>
            </row>
            <row>
              <entry/>
              <entry/>
            </row>
